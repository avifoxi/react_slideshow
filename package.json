{
  "name": "react-slide-v2-front-only",
  "version": "1.0.0",
  "description": "react slideshow with fuller flux architecture - no server, ala flux-shopping-cart example from scotch.io",
  "main": "app.js",
  "scripts": {
    "watch": "watchify js/app.js -o static/bundle.js -v -d",
    "build": "browserify js/app.js -o static/bundle.js"
  },
  "author": "afr",
  "license": "MIT",
  "dependencies": {
    "faker": "^2.1.3",
    "flux": "^2.0.3",
    "lodash": "^3.9.1",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "browserify": "^10.2.1",
    "react-tools": "^0.13.3",
    "reactify": "^1.1.1",
    "watchify": "^3.2.1"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  }
}
